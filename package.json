{
  "name": "fancy-t-spin",
  "version": "0.0.0",
  "main": "./src/build.js",
  "scripts": {
    "test": "npm run eslint",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production npm run build:all",
    "build:all": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html",
    "build:rollup": "rollup -c",
    "build:html": "node ./src/build.js",
    "dev": "rollup  -c -w --no-watch.clearScreen",
    "serve": "node ./src/cleanPublic.js && npm run build:rollup && cross-env NODE_ENV=production node ./src/server.js",
    "esbuild": "node esbuild",
    "eslint": "eslint --fix --ext .js,.mjs,.svelte .",
    "prettier:svelte": "prettier --write --plugin-search-dir=. ./**/*.svelte",
    "preview": "npx sirv-cli docs"
  },
  "type": "commonjs",
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "@elderjs/plugin-browser-reload": "^1.3.4",
    "@elderjs/plugin-seo-check": "^1.3.4",
    "body-parser": "^1.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.1",
    "fumen-svg": "^0.1.5",
    "glob": "^7.2.0",
    "intersection-observer": "^0.12.0",
    "polka": "^0.5.2",
    "relateurl": "^0.2.7",
    "sirv": "^2.0.2",
    "svelte": "^3.46.4",
    "svelte-preprocess": "^4.10.4",
    "urel": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "@elderjs/elderjs": "github:eight04/elderjs#dev-fix",
    "@elderjs/plugin-markdown": "https://gitpkg.now.sh/eight04/plugins-1/packages/markdown?dev-md-data",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.7",
    "rollup": "^2.68.0",
    "typescript": "^4.5.5"
  },
  "author": "eight04 <eight04@gmail.com> (https://github.com/eight04)",
  "license": "MIT",
  "files": [
    "index.js",
    "elderjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eight04/fancy-t-spin.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/eight04/fancy-t-spin/issues"
  },
  "homepage": "https://github.com/eight04/fancy-t-spin#readme"
}
